<div class="form-group">
  <%= f.label :line1, 'Address Line 1:', class: 'col-md-2 control-label' %>
  <div class="col-md-6">
    <%= f.text_field :line1, class: "form-control" %>
    Street address, P.O. Box, company name, c/o
  </div>
</div>

<div class="form-group">
  <%= f.label :line2, 'Address Line 2:', class: 'col-md-2 control-label' %>
  <div class="col-md-6">
    <%= f.text_field :line2, class: "form-control" %>
    Apartment, suite, unit, building, floor, etc.
  </div>
</div>

<div class="form-group">
  <%= f.label :city, 'City:', class: 'col-md-2 control-label' %>
  <div class="col-md-6">
    <%= f.text_field :city, class: "form-control" %>
  </div>
</div>

<div class="form-group">
  <%= f.label :state, 'State:', class: 'col-md-2 control-label' %>
  <div class="col-md-6">
    <%= f.text_field :state, class: "form-control" %>
  </div>
</div>

<div class="form-group">
  <%= f.label :zip, 'Zip Code:', class: 'col-md-2 control-label' %>
  <div class="col-md-6">
    <%= f.text_field :zip, class: "form-control" %>
  </div>
</div>     



